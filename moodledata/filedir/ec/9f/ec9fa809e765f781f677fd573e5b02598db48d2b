      <text>\(a_n=\frac{(3^n-(-1)^n)}{4} \)</text>
      <feedback format="html">
        <text></text>
      </feedback>
    </answer>
    <answer fraction="0" format="html">
      <text>\(a_n = \frac{2^n-(-1)^n)}{3} \)</text>
      <feedback format="html">
        <text></text>
      </feedback>
    </answer>
    <answer fraction="0" format="html">
      <text>\(a_n = \frac{4^n-(-1)^n)}{5} \)</text>
      <feedback format="html">
        <text></text>
      </feedback>
    </answer>
    <answer fraction="0" format="html">
      <text>\(a_n = 2^n \)</text>
      <feedback format="html">
