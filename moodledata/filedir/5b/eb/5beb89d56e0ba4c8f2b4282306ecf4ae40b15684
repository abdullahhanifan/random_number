<!-- question: 29633682  -->
  <question type="matching">
    <name>
      <text>Proposisi 1: Definisi operator</text>
    </name>
    <questiontext format="html">
      <text><![CDATA[<p><strong>EN:</strong> Match the following operators to their definitions.<br></p><p><strong>ID:</strong> Pasangkan operator berikut dengan definisinya.</p>]]></text>
    </questiontext>
    <generalfeedback format="html">
      <text></text>
    </generalfeedback>
    <defaultgrade>1.0000000</defaultgrade>
    <penalty>0.3333333</penalty>
    <hidden>0</hidden>
    <idnumber></idnumber>
    <shuffleanswers>true</shuffleanswers>
    <correctfeedback format="html">
      <text>Your answer is correct.</text>
    </correctfeedback>
    <partiallycorrectfeedback format="html">
      <text>Your answer is partially correct.</text>
    </partiallycorrectfeedback>
    <incorrectfeedback format="html">
      <text>Your answer is incorrect.</text>
    </incorrectfeedback>
    <shownumcorrect/>
    <subquestion format="html">
      <text><![CDATA[<p>\( \land \)</p>]]></text>
      <answer>
        <text>and operator (operator dan)</text>
      </answer>
    </subquestion>
    <subquestion format="html">
      <text><![CDATA[<p>\( \lor \)<br></p>]]></text>
      <answer>
        <text>or operator (operator atau)</text>
      </answer>
    </subquestion>
    <subquestion format="html">
      <text><![CDATA[<p>\( \oplus \)<br></p>]]></text>
      <answer>
        <text>exclusive or (operator atau eksklusif)</text>
      </answer>
    </subquestion>
    <subquestion format="html">
      <text><![CDATA[<p>\( \rightarrow \)<br></p>]]></text>
      <answer>
        <text>if-then operator (operator jika-maka)</text>
      </answer>
    </subquestion>
    <subquestion format="html">
      <text><![CDATA[<p>\( \leftrightarrow \)<br></p>]]></text>
      <answer>
        <text>if and only if operator (operator jika dan hanya jika)</text>
      </answer>
    </subquestion>
    <subquestion format="html">
      <text><![CDATA[<p>\( \neg \)</p>]]></text>
      <answer>
        <text>negation operator (operator negasi)</text>
      </answer>
    </subquestion>
    <tags>
      <tag><text>Logika proposisi 1</text>
</tag>
    </tags>
  </question>

