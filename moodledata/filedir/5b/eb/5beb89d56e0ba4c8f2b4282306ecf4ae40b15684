<!-- question: 29633720  -->
  <question type="multichoice">
    <name>
      <text>Proposisi 1: Preseden</text>
    </name>
    <questiontext format="html">
      <text><![CDATA[<p><strong>EN:</strong> Put additional brackets in \( p \rightarrow \neg q \land r \) to clarify the precedence of its logical operators.</p><p><strong>ID:</strong> Berikan tanda kurung tambahan pada&nbsp; \( p \rightarrow \neg q \land r \) untuk memperjelas presedens dari operator logikanya.<br></p>]]></text>
    </questiontext>
    <generalfeedback format="html">
      <text></text>
    </generalfeedback>
    <defaultgrade>1.0000000</defaultgrade>
    <penalty>0.3333333</penalty>
    <hidden>0</hidden>
    <idnumber></idnumber>
    <single>true</single>
    <shuffleanswers>true</shuffleanswers>
    <answernumbering>none</answernumbering>
    <showstandardinstruction>1</showstandardinstruction>
    <correctfeedback format="html">
      <text>Your answer is correct.</text>
    </correctfeedback>
    <partiallycorrectfeedback format="html">
      <text>Your answer is partially correct.</text>
    </partiallycorrectfeedback>
    <incorrectfeedback format="html">
      <text>Your answer is incorrect.</text>
    </incorrectfeedback>
    <shownumcorrect/>
    <answer fraction="0" format="html">
      <text><![CDATA[<p>\(&nbsp; \left(  p \rightarrow&nbsp; \left( \neg&nbsp; q \right) \right) \land r \)<br></p>]]></text>
      <feedback format="html">
        <text></text>
      </feedback>
    </answer>
    <answer fraction="100" format="html">
      <text><![CDATA[<p>\( p \rightarrow \left( \left( \neg q \right) \land r \right) \)<br></p>]]></text>
      <feedback format="html">
        <text></text>
      </feedback>
    </answer>
    <answer fraction="0" format="html">
      <text><![CDATA[<p>\( p \rightarrow \left( \neg \left( q \land r \right) \right) \)<br></p>]]></text>
      <feedback format="html">
        <text></text>
      </feedback>
    </answer>
