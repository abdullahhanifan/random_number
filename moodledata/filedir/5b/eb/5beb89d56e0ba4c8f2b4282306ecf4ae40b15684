      <feedback format="html">
        <text></text>
      </feedback>
    </answer>
    <answer fraction="-50" format="html">
      <text><![CDATA[<p>p is implied by q (p diakibatkan oleh q)<br></p>]]></text>
      <feedback format="html">
        <text></text>
      </feedback>
    </answer>
