    <subquestion format="html">
      <text><![CDATA[<p><strong>EN:</strong> 2+2 = 4 if and only if 1 + 1 = 2 <br></p><p><strong>ID:</strong> 2+2 = 4 jika dan hanya jika 1+1 = 2</p>]]></text>
      <answer>
        <text>true</text>
      </answer>
    </subquestion>
    <subquestion format="html">
      <text><![CDATA[<p><strong>EN:</strong> 1 + 1 = 2 if and only if 2 + 3 = 4<br></p><p><strong>ID:</strong> 1+1 = 2 jika dan hanya jika 2+3 = 4</p>]]></text>
      <answer>
        <text>false</text>
      </answer>
    </subquestion>
    <subquestion format="html">
      <text><![CDATA[<p><strong>EN:</strong> 1 +1 = 3 if and only if monkeys can fly.<br></p><p><strong>ID:</strong> 1+1 = 3 jika dan hanya jika moyet dapat terbang.</p>]]></text>
      <answer>
        <text>true</text>
      </answer>
    </subquestion>
    <subquestion format="html">
      <text><![CDATA[<p><strong>EN:</strong> 0 &gt; 1 if and only if 2 &gt; 1 <br></p><p><strong>ID:</strong> 0 &gt; 1 jika dan hanya jika 2 &gt; 1</p>]]></text>
      <answer>
        <text>false</text>
      </answer>
    </subquestion>
    <subquestion format="html">
      <text><![CDATA[<p><strong>EN:</strong>if 1 + 1 =3, then unicorn exists. <br></p><p><strong>ID:</strong> Jika 1+1 = 3, maka unicorn memang benar ada.</p>]]></text>
      <answer>
        <text>true</text>
      </answer>
    </subquestion>
    <subquestion format="html">
      <text><![CDATA[<p><strong>EN:</strong> if 1 + 1 = 3, then dogs can fly.</p><p><strong>ID:</strong> Jika 1+1 = 3, maka anjing dapat terbang.<br></p>]]></text>
      <answer>
        <text>true</text>
      </answer>
    </subquestion>
    <subquestion format="html">
      <text><![CDATA[<p><strong>EN:</strong> if 1 + 1 =2, then dogs can fly<br></p><p><strong>ID:</strong> Jika 1+1 = 2, maka anjing dapat terbang.<br></p>]]></text>
      <answer>
        <text>false</text>
      </answer>
    </subquestion>
    <subquestion format="html">
      <text><![CDATA[<p><strong>EN:</strong> if 2 + 2 =4, then 1 + 2 = 3<strong><br></strong></p><p><strong>ID:</strong> jika 2+2 = 4, maka 1+2 = 3</p>]]></text>
      <answer>
        <text>true</text>
      </answer>
    </subquestion>
