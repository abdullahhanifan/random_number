<!-- question: 29633678  -->
  <question type="matching">
    <name>
      <text>Logika Proposisi 1.10</text>
    </name>
    <questiontext format="html">
      <text><![CDATA[<p><strong>EN: </strong>Determine whether the following expression is a propositional formula or not.</p><p><strong>ID:</strong> Tentukan apakah ekspresi berikut ini merupakan formula logika proposisi atau bukan.</p>]]></text>
    </questiontext>
    <generalfeedback format="html">
      <text></text>
    </generalfeedback>
    <defaultgrade>1.0000000</defaultgrade>
    <penalty>0.3333333</penalty>
    <hidden>0</hidden>
    <idnumber></idnumber>
    <shuffleanswers>true</shuffleanswers>
    <correctfeedback format="html">
      <text>Your answer is correct.</text>
    </correctfeedback>
    <partiallycorrectfeedback format="html">
      <text>Your answer is partially correct.</text>
    </partiallycorrectfeedback>
    <incorrectfeedback format="html">
      <text>Your answer is incorrect.</text>
    </incorrectfeedback>
    <shownumcorrect/>
    <subquestion format="html">
      <text><![CDATA[<p>\( p \oplus p \vee q \rightarrow r \wedge s  \)<br></p>]]></text>
      <answer>
        <text>propositional formula (formula logika proposisi)</text>
      </answer>
    </subquestion>
    <subquestion format="html">
      <text><![CDATA[<p>\(  \sim \sim ( \sim p \rightarrow \sim \sim r)   \)<br></p>]]></text>
      <answer>
        <text>propositional formula (formula logika proposisi)</text>
      </answer>
    </subquestion>
    <subquestion format="html">
      <text><![CDATA[<p>\( p \wedge q \rightarrow \oplus r \vee s  \)<br></p>]]></text>
      <answer>
        <text>not a propositional formula (bukan formula logika proposisi)</text>
      </answer>
    </subquestion>
    <subquestion format="html">
      <text><![CDATA[<p>\( p \vee q \vee \rightarrow r \oplus s  \)<br></p>]]></text>
      <answer>
        <text>not a propositional formula (bukan formula logika proposisi)</text>
      </answer>
    </subquestion>
    <subquestion format="html">
      <text><![CDATA[<p>\(  \sim ( (p \rightarrow q) \sim (r \oplus s) )  \)<br></p>]]></text>
      <answer>
        <text>not a propositional formula (bukan formula logika proposisi)</text>
      </answer>
    </subquestion>
    <subquestion format="html">
      <text><![CDATA[<p>\( p \wedge q \sim r \)<br></p>]]></text>
      <answer>
        <text>not a propositional formula (bukan formula logika proposisi)</text>
      </answer>
    </subquestion>
    <subquestion format="html">
      <text><![CDATA[<p>\( p \oplus q \ \leftrightarrow r \vee \sim t \)<br></p>]]></text>
      <answer>
        <text>propositional formula (formula logika proposisi)</text>
      </answer>
    </subquestion>
    <subquestion format="html">
      <text><![CDATA[<p>\( \sim p \rightarrow \sim p \oplus q \)<br></p>]]></text>
      <answer>
        <text>propositional formula (formula logika proposisi)</text>
      </answer>
    </subquestion>
    <tags>
      <tag><text>Logika proposisi 1</text>
</tag>
    </tags>
  </question>

